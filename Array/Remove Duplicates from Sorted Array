class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        set<int>s;
        for(auto i:nums)
        {
            s.insert(i);  // use of set because remove duplicates 
        }
        nums.assign(s.begin(), s.end()); // make sure you insert back to the nums vector.
        return nums.size();
    }
};
