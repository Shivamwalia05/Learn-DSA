class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int j=0; // point to the nums[0].
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]!=0)
            {
                swap(nums[j],nums[i]); // swap when nums[i] is not zero .
                j++; // move agter swap .
            }
           
        }
    }
};
